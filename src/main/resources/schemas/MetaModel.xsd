	<xsd:simpleType name="CPUArchitectureType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="INTEL" />
			<xsd:enumeration value="AMD" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RAMTypeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="DDR" />
			<xsd:enumeration value="DDR2" />
			<xsd:enumeration value="DDR3" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ServerStatusType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ON" />
			<xsd:enumeration value="OFF" />
			<xsd:enumeration value="STANDBY" />
			<xsd:enumeration value="POWERING_ON"/>
			<xsd:enumeration value="POWERING_OFF"/>
		</xsd:restriction>
	</xsd:simpleType>
        <xsd:simpleType name="NetworkNodeStatusType">
                <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="ON" />
                        <xsd:enumeration value="OFF" />
                        <xsd:enumeration value="STANDBY" />
                </xsd:restriction>
        </xsd:simpleType>
	<xsd:simpleType name="NodePropType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="TO_BE_REVISED" />
			<xsd:enumeration value="PROP2" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NodeStatusType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="DOWN" />
			<xsd:enumeration value="IDLE" />
			<xsd:enumeration value="RESERVED" />
			<xsd:enumeration value="BUSY" />
			<xsd:enumeration value="RUNNING" />
			<xsd:enumeration value="STANDBY" />
			<xsd:enumeration value="HYBERNATED" />
			<xsd:enumeration value="OFF" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="JobPropOfNodesType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="TO_BE_REVISED" />
			<xsd:enumeration value="PROP2" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="JobStatusType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="QUEUED" />
			<xsd:enumeration value="RUNNING" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="QueuePossiblePropType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="TO_BE_REVISED" />
			<xsd:enumeration value="PROP2" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="QueueForcedPropType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="TO_BE_REVISED" />
			<xsd:enumeration value="PROP2" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ServerRoleType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="HPC_COMPUTE_NODE" />
			<xsd:enumeration value="HPC_RESOURCE_MANAGEMENT" />
			<xsd:enumeration value="CLOUD_CONTROLLER" />
			<xsd:enumeration value="CLOUD_CLUSTER_CONTROLLER" />
			<xsd:enumeration value="CLOUD_NODE_CONTROLLER" />
			<xsd:enumeration value="TRADITIONAL_HOST" />
			<xsd:enumeration value="OTHER" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DCComputingStyleType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="SUPER" />
			<xsd:enumeration value="TRADITIONAL" />
			<xsd:enumeration value="CLOUD" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RAMTypeVendorType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="KINGSTON" />
			<xsd:enumeration value="SAMSUNG" />
			<xsd:enumeration value="HYNIX" />
			<xsd:enumeration value="GENERIC" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="OperatingSystemTypeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="WINDOWS" />
			<xsd:enumeration value="LINUX" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="NativeHypervisorNameType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ESX" />
			<xsd:enumeration value="Hyper-V" />
			<xsd:enumeration value="XenServer" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="HostedHypervisorNameType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="VirtualPC" />
			<xsd:enumeration value="VirtualBox" />
			<xsd:enumeration value="KVM" />
			<xsd:enumeration value="Xen" />
			<xsd:enumeration value="VMWare" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="BufferTypeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Buffered" />
			<xsd:enumeration value="Unbuffered" />
			<xsd:enumeration value="Registered" />
			<xsd:enumeration value="Fully Buffered" />
			<xsd:enumeration value="Other" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="FrameworkStatusType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Starting" />
			<xsd:enumeration value="Running" />
			<xsd:enumeration value="Stopped" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="PowerType"> <!--Watt -->
		<xsd:restriction base="xsd:double" />
	</xsd:simpleType>
	
	<xsd:simpleType name="HitRatioType"> <!--Percent [0,1] -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
			<xsd:maxInclusive value="1" />
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="VoltageType"> <!--Volt -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="FrequencyType"> <!--For CPU/Core, it is in GHz and for RAM, it is in MHz -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>	
	<xsd:simpleType name="StorageCapacityType"> <!--Byte-->
    <xsd:restriction base="xsd:double">
      <xsd:minInclusive value="0"/>
    </xsd:restriction> 
  </xsd:simpleType>
	<xsd:simpleType name="IoRateType"> <!--Operation/Second -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BlockSizeType"> <!--kB -->
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="EfficiencyType">
		<xsd:restriction base="xsd:double" />
	</xsd:simpleType>
	<xsd:simpleType name="CoreLoadType"> <!--percentage of NON-idle mode cycles (from 0.0 to 100.0) -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BandwidthType"> <!--bits/second -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NetworkTrafficType"> <!-- packet/second -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CpuUsageType"> <!-- percentage of NON-idle mode cycles, separate values for each cpu; this 
			value will be “spread” evenly on all cores of the respective cpu; only if 
			core load is not provided -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StorageUsageType"> <!--GigaByte -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="MemoryUsageType"> <!--GigaByte allocated system memory by OS -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RAMSizeType"> <!--GigaByte -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NetworkUsageType"> <!-- packet/second -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PUEType">
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="1" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CUEType">
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LithographyType">
		<xsd:restriction base="xsd:int" />
	</xsd:simpleType>
	<xsd:simpleType name="IntPercentType">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
			<xsd:maxInclusive value="100" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RPMType">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>	
	<xsd:simpleType name="NrOfCpusType">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NrOfPstatesType">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NrOfTransistorsType">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>	
	<xsd:simpleType name="PSULoadType">    <!-- Note: this is a percentage -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
			<xsd:maxInclusive value="100" />
		</xsd:restriction>
	</xsd:simpleType>	
	<xsd:simpleType name="DimensionType">
		<xsd:restriction base="xsd:int" />
	</xsd:simpleType>
	<xsd:simpleType name="RAIDLevelType">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="OPType" >
    	<xsd:restriction base="xsd:int">
      		<xsd:minInclusive value="1"/>
    	</xsd:restriction>
  	</xsd:simpleType>
  	<xsd:simpleType name="StripSizeType" >
    	<xsd:restriction base="xsd:double">
      		<xsd:minInclusive value="0"/>
    	</xsd:restriction>
  	</xsd:simpleType>	
	<xsd:simpleType name="CacheLevelType">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NrOfPortsType">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NrOfLinksType">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NrOfPlugsType">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NetworkPortBufferSizeType">
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NetworkPortBufferOccupancyType">
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="FileSystemFragmentationType">
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="FileSystemSpaceType"> <!-- TBD: how is this expressed (bytes, MB, GB)? see type where it's used also -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SwitchFabricType">
		<xsd:restriction base="xsd:int" />
	</xsd:simpleType>
	<xsd:simpleType name="NrOfNodesType">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NrOfCoresType">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="JobPriorityType">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="JobTimeType">
		<xsd:restriction base="xsd:long">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="JobLimitType">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="QueuePriorityType">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PropagationDelayType">
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BitErrorRateType">
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ScenarioType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="single" />
			<xsd:enumeration value="federated" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LocationType">
		<xsd:restriction base="xsd:string" />
	</xsd:simpleType>

	<xsd:complexType name="FIT4GreenType">
		<xsd:sequence>
			<xsd:element ref="f4g:Site" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element name="description" type="xsd:string"
				minOccurs="0" maxOccurs="unbounded" />
			<xsd:element name="version" type="xsd:string" minOccurs="0"
				maxOccurs="unbounded" />
			<xsd:element name="Datetime" type="xsd:dateTime" minOccurs="0" 
				maxOccurs= "1" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SiteType">
		<xsd:sequence>
		  	<xsd:element name="name" type="xsd:string" minOccurs="0"
				maxOccurs="1" />
			<xsd:element name="PUE" type="f4g:PUEType" minOccurs="1"
				maxOccurs="1" />
			<xsd:element name="CUE" type="f4g:CUEType" minOccurs="1"
				maxOccurs="1" />
			<xsd:element name="computedPower" type="f4g:PowerType"
				minOccurs="0" maxOccurs="1" />
			<xsd:element ref="f4g:Datacenter" minOccurs="0"
				maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DatacenterType">
		<xsd:sequence>
			<xsd:element name="name" type="xsd:string" minOccurs="0"
				maxOccurs="1" />
			<xsd:element name="computingStyle" type="f4g:DCComputingStyleType"
				minOccurs="1" maxOccurs="1" />
			<xsd:element ref="f4g:ApplicationBenchmark" minOccurs="0" maxOccurs="unbounded" />				
			<xsd:element name="computedPower" type="f4g:PowerType"
				minOccurs="0" maxOccurs="1" />
			<xsd:element ref="f4g:Rack" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element ref="f4g:TowerServer" minOccurs="0"
				maxOccurs="unbounded" />
			<xsd:element ref="f4g:BoxRouter" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element ref="f4g:BoxSwitch" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element ref="f4g:FrameworkCapabilities" minOccurs="0"
				maxOccurs="unbounded" />
			<xsd:element ref="f4g:NetworkLoad" minOccurs="0"
				maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RackType">
		<xsd:sequence>
			<xsd:element name="computedPower" type="f4g:PowerType"
				minOccurs="0" maxOccurs="1" />
			<xsd:element ref="f4g:RackableServer" minOccurs="0"
				maxOccurs="unbounded" />
			<xsd:element ref="f4g:Fan" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element ref="f4g:WaterCooler" minOccurs="0"
				maxOccurs="unbounded" />
			<xsd:element ref="f4g:RackableRouter" minOccurs="0"
				maxOccurs="unbounded" />
			<xsd:element ref="f4g:RackableSwitch" minOccurs="0"
				maxOccurs="unbounded" />
			<xsd:element ref="f4g:SAN" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element ref="f4g:NAS" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="f4g:Enclosure" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element ref="f4g:PDU" minOccurs="0" maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="ServerType">
	<xsd:sequence>
		<xsd:element name="name" type="f4g:ServerRoleType"
			minOccurs="1" maxOccurs="1" />
		<xsd:element name="status" type="f4g:ServerStatusType"
