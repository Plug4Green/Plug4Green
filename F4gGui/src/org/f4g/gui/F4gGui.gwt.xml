<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='f4ggui'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.standard.Standard'/>
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits                                      -->
  <inherits name='com.google.gwt.visualization.Visualization'/> 
  <inherits name="com.google.gwt.json.JSON" />
  <inherits name='com.chap.links.Graph'/>
  <inherits name='com.chap.links.Timeline'/>
  <inherits name='com.chap.links.Network'/>

  <!-- External stylesheets                                       -->
  <!-- Include at least one stylesheet, else the stylesheets in   -->
  <!-- external modules like gwt-links-graph.jar are not loaded...-->
  <stylesheet src="graph_style.css"></stylesheet>
  <stylesheet src="timeline_style.css"></stylesheet>

  <!-- Specify the app entry point class.                         -->
  <entry-point class='org.f4g.gui.client.F4gGui'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>

</module>
