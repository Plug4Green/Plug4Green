<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema xmlns:f4g="f4g/schemas/java/MetaModel"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	jaxb:version="2.0" targetNamespace="f4g/schemas/java/MetaModel">
	<xsd:annotation>
		<xsd:appinfo>
			<jaxb:globalBindings mapSimpleTypeDef="true" />
		</xsd:appinfo>
	</xsd:annotation>

	<xsd:element name="FIT4Green" type="f4g:FIT4Green" />
	<xsd:element name="Site" type="f4g:Site" /> 
	<xsd:element name="Datacenter" type="f4g:Datacenter" />
	<xsd:element name="Rack" type="f4g:Rack" />
	<xsd:element name="Server" type="f4g:Server" />
	<xsd:element name="CPU" type="f4g:CPU" />
	<xsd:element name="Controller" type="f4g:Controller" />
	<xsd:element name="RAMStick" type="f4g:RAMStick" />
	<xsd:element name="HardDisk" type="f4g:HardDisk" />
	<xsd:element name="StorageUnit" type="f4g:StorageUnit" />
	<xsd:element name="Mainboard" type="f4g:Mainboard" />
	<xsd:element name="PSU" type="f4g:PSU" />
	<xsd:element name="Fan" type="f4g:Fan" />
	<xsd:element name="RAID" type="f4g:RAID" />
	<xsd:element name="RAIDDisk" type="f4g:RAIDDisk" />
	<xsd:element name="Cache" type="f4g:Cache" />
	<xsd:element name="Core" type="f4g:Core" />
	<xsd:element name="SolidStateDisk" type="f4g:SolidStateDisk" />
	<xsd:element name="TowerServer" type="f4g:TowerServer" />
	<xsd:element name="BladeServer" type="f4g:BladeServer" />
	<xsd:element name="RackableServer" type="f4g:RackableServer" />
	<xsd:element name="Enclosure" type="f4g:Enclosure" />
	<xsd:element name="SAN" type="f4g:SAN" />
	<xsd:element name="NAS" type="f4g:NAS" />
	<xsd:element name="PDU" type="f4g:PDU" />
	<xsd:element name="NetworkNode" type="f4g:NetworkNode" />
	<xsd:element name="EthernetNIC" type="f4g:NIC" />
	<xsd:element name="FiberchannelNIC" type="f4g:NIC" />
	<xsd:element name="GPU" type="f4g:GPU" />	
	<xsd:element name="NetworkPort" type="f4g:NetworkPort" />
	<xsd:element name="ATM" type="f4g:ATM" />
	<xsd:element name="VPN" type="f4g:VPN" />
	<xsd:element name="BoxSwitch" type="f4g:BoxSwitch" />
	<xsd:element name="BoxRouter" type="f4g:BoxRouter" />
	<xsd:element name="RackableSwitch" type="f4g:RackableSwitch" />
	<xsd:element name="RackableRouter" type="f4g:RackableRouter" />
	<xsd:element name="NativeOperatingSystem" type="f4g:NativeOperatingSystem" />
	<xsd:element name="OperatingSystem" type="f4g:OperatingSystem" />
	<xsd:element name="HostedHypervisor" type="f4g:HostedHypervisor" />
	<xsd:element name="NativeHypervisor" type="f4g:NativeHypervisor" />
	<xsd:element name="VirtualMachine" type="f4g:VirtualMachine" />
	<xsd:element name="FileSystem" type="f4g:FileSystem" />
	<xsd:element name="HostedOperatingSystem" type="f4g:HostedOperatingSystem" />
	<xsd:element name="SoftwareNetwork" type="f4g:SoftwareNetwork" />
	<xsd:element name="SoftwareRAID" type="f4g:SoftwareRAID" />
	<xsd:element name="HardwareRAID" type="f4g:HardwareRAID" />
	<xsd:element name="SoftwareApplication" type="f4g:SoftwareApplication" />
	<xsd:element name="e80211x" type="f4g:e80211x" />
	<xsd:element name="Ethernet" type="f4g:Ethernet" />
	<xsd:element name="Tunnel" type="f4g:Tunnel" />
	<xsd:element name="SerialPPP" type="f4g:SerialPPP" />
	<xsd:element name="OpticalFDDI" type="f4g:OpticalFDDI" />	
	<xsd:element name="Job" type="f4g:Job" />
	<xsd:element name="Queue" type="f4g:Queue" />
	<xsd:element name="Role" type="f4g:Role" />
	<xsd:element name="Node" type="f4g:Node" />
	<xsd:element name="ClusterManagement" type="f4g:ClusterManagement" />
	<xsd:element name="FrameworkCapabilities" type="f4g:FrameworkCapabilities" />
	<xsd:element name="NodeActions" type="f4g:NodeActions" />
	<xsd:element name="VMActions" type="f4g:VMActions" />
	<xsd:element name="JobActions" type="f4g:JobActions" />
	<xsd:element name="WaterCooler" type="f4g:WaterCooler" />
	<xsd:element name="Link" type="f4g:Link" />
	<xsd:element name="NetworkLoad" type="f4g:NetworkLoad" />
	<xsd:element name="Flow" type="f4g:Flow" />
	<xsd:element name="LogicalUnit" type="f4g:LogicalUnit" />
	<xsd:element name="LogicalVolume" type="f4g:LogicalVolume" />
	
	<xsd:element name="LUN" type="f4g:LUN" />
	<xsd:element name="ApplicationBenchmark" type="f4g:ApplicationBenchmark" />

	<xsd:simpleType name="CPUArchitecture">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="INTEL" />
			<xsd:enumeration value="AMD" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RAMTypeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="DDR" />
			<xsd:enumeration value="DDR2" />
			<xsd:enumeration value="DDR3" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ServerStatus">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ON" />
			<xsd:enumeration value="OFF" />
			<xsd:enumeration value="STANDBY" />
			<xsd:enumeration value="POWERING_ON"/>
			<xsd:enumeration value="POWERING_OFF"/>
		</xsd:restriction>
	</xsd:simpleType>
        <xsd:simpleType name="NetworkNodeStatus">
                <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="ON" />
                        <xsd:enumeration value="OFF" />
                        <xsd:enumeration value="STANDBY" />
                </xsd:restriction>
        </xsd:simpleType>
	<xsd:simpleType name="NodeProp">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="TO_BE_REVISED" />
			<xsd:enumeration value="PROP2" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NodeStatus">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="DOWN" />
			<xsd:enumeration value="IDLE" />
			<xsd:enumeration value="RESERVED" />
			<xsd:enumeration value="BUSY" />
			<xsd:enumeration value="RUNNING" />
			<xsd:enumeration value="STANDBY" />
			<xsd:enumeration value="HYBERNATED" />
			<xsd:enumeration value="OFF" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="JobPropOfNodes">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="TO_BE_REVISED" />
			<xsd:enumeration value="PROP2" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="JobStatus">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="QUEUED" />
			<xsd:enumeration value="RUNNING" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="QueuePossibleProp">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="TO_BE_REVISED" />
			<xsd:enumeration value="PROP2" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="QueueForcedProp">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="TO_BE_REVISED" />
			<xsd:enumeration value="PROP2" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ServerRole">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="HPC_COMPUTE_NODE" />
			<xsd:enumeration value="HPC_RESOURCE_MANAGEMENT" />
			<xsd:enumeration value="CLOUD_CONTROLLER" />
			<xsd:enumeration value="CLOUD_CLUSTER_CONTROLLER" />
			<xsd:enumeration value="CLOUD_NODE_CONTROLLER" />
			<xsd:enumeration value="TRADITIONAL_HOST" />
			<xsd:enumeration value="OTHER" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RAMTypeVendorType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="KINGSTON" />
			<xsd:enumeration value="SAMSUNG" />
			<xsd:enumeration value="HYNIX" />
			<xsd:enumeration value="GENERIC" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="OperatingSystemType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="WINDOWS" />
			<xsd:enumeration value="LINUX" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="NativeHypervisorName">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ESX" />
			<xsd:enumeration value="Hyper-V" />
			<xsd:enumeration value="XenServer" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="HostedHypervisorName">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="VirtualPC" />
			<xsd:enumeration value="VirtualBox" />
			<xsd:enumeration value="KVM" />
			<xsd:enumeration value="Xen" />
			<xsd:enumeration value="VMWare" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="BufferTypeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Buffered" />
			<xsd:enumeration value="Unbuffered" />
			<xsd:enumeration value="Registered" />
			<xsd:enumeration value="Fully Buffered" />
			<xsd:enumeration value="Other" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="FrameworkStatus">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Starting" />
			<xsd:enumeration value="Running" />
			<xsd:enumeration value="Stopped" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="Power"> <!--Watt -->
		<xsd:restriction base="xsd:double" />
	</xsd:simpleType>
	
	<xsd:simpleType name="HitRatio"> <!--Percent [0,1] -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
			<xsd:maxInclusive value="1" />
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="Voltage"> <!--Volt -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Frequency"> <!--For CPU/Core, it is in GHz and for RAM, it is in MHz -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>	
	<xsd:simpleType name="StorageCapacity"> <!--Byte-->
    <xsd:restriction base="xsd:double">
      <xsd:minInclusive value="0"/>
    </xsd:restriction> 
  </xsd:simpleType>
	<xsd:simpleType name="IoRate"> <!--Operation/Second -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BlockSize"> <!--kB -->
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Efficiency">
		<xsd:restriction base="xsd:double" />
	</xsd:simpleType>
	<xsd:simpleType name="CoreLoad"> <!--percentage of NON-idle mode cycles (from 0.0 to 100.0) -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Bandwidth"> <!--bits/second -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NetworkTraffic"> <!-- packet/second -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CpuUsage"> <!-- percentage of NON-idle mode cycles, separate values for each cpu; this 
			value will be “spread” evenly on all cores of the respective cpu; only if 
			core load is not provided -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StorageUsage"> <!--GigaByte -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="MemoryUsage"> <!--GigaByte allocated system memory by OS -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RAMSize"> <!--GigaByte -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NetworkUsage"> <!-- packet/second -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PUE">
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="1" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CUE">
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Lithography">
		<xsd:restriction base="xsd:int" />
	</xsd:simpleType>
	<xsd:simpleType name="IntPercent">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
			<xsd:maxInclusive value="100" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RPM">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>	
	<xsd:simpleType name="NrOfCpus">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NrOfPstates">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NrOfTransistors">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>	
	<xsd:simpleType name="PSULoad">    <!-- Note: this is a percentage -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
			<xsd:maxInclusive value="100" />
		</xsd:restriction>
	</xsd:simpleType>	
	<xsd:simpleType name="Dimension">
		<xsd:restriction base="xsd:int" />
	</xsd:simpleType>
	<xsd:simpleType name="RAIDLevel">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="OP" >
    	<xsd:restriction base="xsd:int">
      		<xsd:minInclusive value="1"/>
    	</xsd:restriction>
  	</xsd:simpleType>
  	<xsd:simpleType name="StripSize" >
    	<xsd:restriction base="xsd:double">
      		<xsd:minInclusive value="0"/>
    	</xsd:restriction>
  	</xsd:simpleType>	
	<xsd:simpleType name="CacheLevel">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NrOfPorts">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NrOfLinks">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NrOfPlugs">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NetworkPortBufferSize">
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NetworkPortBufferOccupancy">
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="FileSystemFragmentation">
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="FileSystemSpace"> <!-- TBD: how is this expressed (bytes, MB, GB)? see type where it's used also -->
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SwitchFabric">
		<xsd:restriction base="xsd:int" />
	</xsd:simpleType>
	<xsd:simpleType name="NrOfNodes">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NrOfCores">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="JobPriority">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="JobTime">
		<xsd:restriction base="xsd:long">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="JobLimit">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="QueuePriority">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PropagationDelay">
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BitErrorRate">
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Scenario">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="single" />
			<xsd:enumeration value="federated" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Location">
		<xsd:restriction base="xsd:string" />
	</xsd:simpleType>

	<xsd:complexType name="FIT4Green">
		<xsd:sequence>
			<xsd:element ref="f4g:Site" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element name="description" type="xsd:string"
				minOccurs="0" maxOccurs="unbounded" />
			<xsd:element name="version" type="xsd:string" minOccurs="0"
				maxOccurs="unbounded" />
			<xsd:element name="Datetime" type="xsd:dateTime" minOccurs="0" 
				maxOccurs= "1" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Site">
		<xsd:sequence>
		  	<xsd:element name="name" type="xsd:string" minOccurs="0"
				maxOccurs="1" />
			<xsd:element name="PUE" type="f4g:PUE" minOccurs="1"
				maxOccurs="1" />
			<xsd:element name="CUE" type="f4g:CUE" minOccurs="1"
				maxOccurs="1" />
			<xsd:element name="computedPower" type="f4g:Power"
				minOccurs="0" maxOccurs="1" />
			<xsd:element ref="f4g:Datacenter" minOccurs="0"
				maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Datacenter">
		<xsd:sequence>
			<xsd:element name="name" type="xsd:string" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="f4g:ApplicationBenchmark" minOccurs="0" maxOccurs="unbounded" />				
			<xsd:element name="computedPower" type="f4g:Power"
				minOccurs="0" maxOccurs="1" />
			<xsd:element ref="f4g:Rack" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element ref="f4g:TowerServer" minOccurs="0"
				maxOccurs="unbounded" />
			<xsd:element ref="f4g:BoxRouter" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element ref="f4g:BoxSwitch" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element ref="f4g:FrameworkCapabilities" minOccurs="0"
				maxOccurs="unbounded" />
			<xsd:element ref="f4g:NetworkLoad" minOccurs="0"
				maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Rack">
		<xsd:sequence>
			<xsd:element name="computedPower" type="f4g:Power"
				minOccurs="0" maxOccurs="1" />
			<xsd:element ref="f4g:RackableServer" minOccurs="0"
				maxOccurs="unbounded" />
			<xsd:element ref="f4g:Fan" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element ref="f4g:WaterCooler" minOccurs="0"
				maxOccurs="unbounded" />
			<xsd:element ref="f4g:RackableRouter" minOccurs="0"
				maxOccurs="unbounded" />
			<xsd:element ref="f4g:RackableSwitch" minOccurs="0"
				maxOccurs="unbounded" />
			<xsd:element ref="f4g:SAN" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element ref="f4g:NAS" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="f4g:Enclosure" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element ref="f4g:PDU" minOccurs="0" maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="Server">
	<xsd:sequence>
		<xsd:element name="name" type="f4g:ServerRole"
			minOccurs="1" maxOccurs="1" />
		<xsd:element name="status" type="f4g:ServerStatus"
			minOccurs="1" maxOccurs="1" />
		<xsd:element name="frameworkID" type="xsd:string"
			minOccurs="1" maxOccurs="1" />
		<xsd:element name="computedPower" type="f4g:Power"
			minOccurs="0" maxOccurs="1" />
		<xsd:element name="measuredPower" type="f4g:Power"
			minOccurs="0" maxOccurs="1" />
		<xsd:element ref="f4g:LogicalVolume" minOccurs="0" maxOccurs="unbounded" />
		<xsd:element ref="f4g:Mainboard" minOccurs="0" maxOccurs="unbounded" />
		<xsd:choice minOccurs="0" maxOccurs="1">
			<xsd:element ref="f4g:NativeOperatingSystem" minOccurs="1"
				maxOccurs="1" />
			<xsd:element ref="f4g:NativeHypervisor" minOccurs="1"
				maxOccurs="1" />
		</xsd:choice>
		<xsd:element name="lastOnOffTimestamp" type="xsd:dateTime"
