				minOccurs="0" maxOccurs="1" />
			<xsd:element name="actualStorageUsage" type="f4g:StorageUsage"
				minOccurs="0" maxOccurs="1" />
			<xsd:element name="actualDiskIORate" type="f4g:IoRate"
				minOccurs="0" maxOccurs="1" />
			<xsd:element name="actualMemoryUsage" type="f4g:MemoryUsage"
				minOccurs="0" maxOccurs="1" />
